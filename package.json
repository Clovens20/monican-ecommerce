{
  "name": "monican_ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "dev:clean": "node -e \"const fs=require('fs');const path='.next/dev/lock';try{fs.unlinkSync(path);console.log('Lock file removed')}catch(e){}\" && next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start -H 0.0.0.0",
    "preview": "next start -H 0.0.0.0",
    "lint": "eslint",
